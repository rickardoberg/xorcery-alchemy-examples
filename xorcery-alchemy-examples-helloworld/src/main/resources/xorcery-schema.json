            "sources" : {
              "type" : [ "array", "string" ],
              "items" : {
                "$ref" : "#/$defs/sourcejar"
              }
            }
          }
        } ]
      }, {
        "allOf" : [ {
          "$ref" : "#/$defs/jar"
        }, {
          "properties" : {
            "jar" : {
              "const" : "yaml"
            },
            "file" : {
              "type" : "string",
              "description" : "Resource name in project or URL to YAML file"
            }
          }
        } ]
      } ]
